        "fieldConfig": {
          "defaults": {
            "color": { "mode": "palette-classic" },
            "custom": {
              "axisCenteredZero": false,
              "axisColorMode": "text",
              "axisLabel": "Memory (MB)",
              "axisPlacement": "auto",
              "drawStyle": "line",
              "fillOpacity": 20,
              "lineInterpolation": "smooth",
              "lineWidth": 1,
              "showPoints": "never",
              "spanNulls": true,
              "stacking": { "group": "A", "mode": "normal" },
              "thresholdsStyle": { "mode": "off" }
