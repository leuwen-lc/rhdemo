      },
      {
        "id": 30,
        "title": "HTTP Requests",
        "type": "row",
        "gridPos": { "x": 0, "y": 31, "w": 24, "h": 1 },
        "collapsed": false
      },
      {
        "id": 31,
        "title": "Requetes HTTP/sec par Status",
        "type": "timeseries",
        "datasource": { "type": "prometheus", "uid": "prometheus" },
        "gridPos": { "x": 0, "y": 32, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "sum by (status) (rate(http_server_requests_seconds_count{environment=\"stagingkub\"}[5m]))",
            "refId": "A",
