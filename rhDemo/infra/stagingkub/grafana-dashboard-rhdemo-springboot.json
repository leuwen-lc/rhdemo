{
  "dashboard": {
    "uid": "rhdemo-springboot",
    "title": "rhDemo - Metriques Spring Boot Actuator",
    "description": "Dashboard des metriques JVM, HTTP et HikariCP exposees par Spring Boot Actuator via /actuator/prometheus",
    "tags": ["prometheus", "rhdemo", "spring-boot", "actuator", "jvm", "hikaricp"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    },
    "panels": [
      {
        "id": 1,
        "title": "Vue d'ensemble JVM",
        "type": "row",
        "gridPos": { "x": 0, "y": 0, "w": 24, "h": 1 },
        "collapsed": false
      },
      {
        "id": 2,
        "title": "JVM Heap Usage",
        "type": "stat",
        "datasource": { "type": "prometheus", "uid": "prometheus" },
        "gridPos": { "x": 0, "y": 1, "w": 6, "h": 4 },
        "targets": [
          {
            "expr": "sum(jvm_memory_used_bytes{environment=\"stagingkub\", area=\"heap\"}) / sum(jvm_memory_max_bytes{environment=\"stagingkub\", area=\"heap\"}) * 100",
            "refId": "A",
            "legendFormat": "Heap %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": { "mode": "thresholds" },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": null, "color": "green" },
                { "value": 70, "color": "yellow" },
                { "value": 85, "color": "red" }
              ]
            },
            "unit": "percent",
            "decimals": 1
          }
        },
        "options": {
          "reduceOptions": { "values": false, "calcs": ["lastNotNull"] },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto"
        }
      },
      {
        "id": 3,
        "title": "JVM Threads",
        "type": "stat",
        "datasource": { "type": "prometheus", "uid": "prometheus" },
        "gridPos": { "x": 6, "y": 1, "w": 6, "h": 4 },
        "targets": [
          {
            "expr": "jvm_threads_live_threads{environment=\"stagingkub\"}",
            "refId": "A",
            "legendFormat": "Live Threads"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": { "mode": "thresholds" },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": null, "color": "green" },
                { "value": 100, "color": "yellow" },
                { "value": 200, "color": "red" }
              ]
            },
            "unit": "short",
            "decimals": 0
          }
        },
        "options": {
          "reduceOptions": { "values": false, "calcs": ["lastNotNull"] },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto"
        }
      },
      {
        "id": 4,
        "title": "HTTP Requests/sec",
        "type": "stat",
        "datasource": { "type": "prometheus", "uid": "prometheus" },
        "gridPos": { "x": 12, "y": 1, "w": 6, "h": 4 },
        "targets": [
          {
            "expr": "sum(rate(http_server_requests_seconds_count{environment=\"stagingkub\"}[5m]))",
            "refId": "A",
            "legendFormat": "req/s"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": { "mode": "thresholds" },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": null, "color": "blue" },
                { "value": 10, "color": "green" },
                { "value": 100, "color": "yellow" }
              ]
            },
            "unit": "reqps",
            "decimals": 2
          }
        },
        "options": {
          "reduceOptions": { "values": false, "calcs": ["lastNotNull"] },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto"
        }
      },
      {
        "id": 5,
        "title": "DB Connections Active",
        "type": "stat",
        "datasource": { "type": "prometheus", "uid": "prometheus" },
        "gridPos": { "x": 18, "y": 1, "w": 6, "h": 4 },
        "targets": [
          {
            "expr": "hikaricp_connections_active{environment=\"stagingkub\"}",
            "refId": "A",
            "legendFormat": "Active"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": { "mode": "thresholds" },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": null, "color": "green" },
                { "value": 5, "color": "yellow" },
                { "value": 8, "color": "red" }
              ]
            },
            "unit": "short",
            "decimals": 0
          }
        },
        "options": {
          "reduceOptions": { "values": false, "calcs": ["lastNotNull"] },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto"
        }
      },
      {
        "id": 10,
        "title": "JVM Memory",
        "type": "row",
        "gridPos": { "x": 0, "y": 5, "w": 24, "h": 1 },
        "collapsed": false
      },
      {
        "id": 11,
        "title": "JVM Heap Memory",
        "type": "timeseries",
        "datasource": { "type": "prometheus", "uid": "prometheus" },
        "gridPos": { "x": 0, "y": 6, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "sum(jvm_memory_used_bytes{environment=\"stagingkub\", area=\"heap\"}) / 1024 / 1024",
            "refId": "A",
            "legendFormat": "Used"
          },
          {
            "expr": "sum(jvm_memory_committed_bytes{environment=\"stagingkub\", area=\"heap\"}) / 1024 / 1024",
            "refId": "B",
            "legendFormat": "Committed"
          },
          {
            "expr": "sum(jvm_memory_max_bytes{environment=\"stagingkub\", area=\"heap\"}) / 1024 / 1024",
            "refId": "C",
            "legendFormat": "Max"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": { "mode": "palette-classic" },
            "custom": {
              "axisCenteredZero": false,
              "axisColorMode": "text",
              "axisLabel": "Memory (MB)",
              "axisPlacement": "auto",
              "drawStyle": "line",
              "fillOpacity": 10,
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "showPoints": "never",
              "spanNulls": true,
              "stacking": { "group": "A", "mode": "none" },
              "thresholdsStyle": { "mode": "off" }
            },
            "unit": "decmbytes",
            "decimals": 0
          },
          "overrides": [
            {
              "matcher": { "id": "byName", "options": "Max" },
              "properties": [
                { "id": "custom.lineStyle", "value": { "fill": "dash", "dash": [10, 10] } },
                { "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }
              ]
            },
            {
              "matcher": { "id": "byName", "options": "Committed" },
              "properties": [
                { "id": "custom.lineStyle", "value": { "fill": "dash", "dash": [5, 5] } },
                { "id": "color", "value": { "fixedColor": "yellow", "mode": "fixed" } }
              ]
            }
          ]
        },
        "options": {
          "legend": { "showLegend": true, "displayMode": "table", "placement": "right", "calcs": ["mean", "max"] },
          "tooltip": { "mode": "multi", "sort": "desc" }
        }
      },
      {
        "id": 12,
        "title": "JVM Non-Heap Memory",
        "type": "timeseries",
        "datasource": { "type": "prometheus", "uid": "prometheus" },
        "gridPos": { "x": 12, "y": 6, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "sum(jvm_memory_used_bytes{environment=\"stagingkub\", area=\"nonheap\"}) / 1024 / 1024",
            "refId": "A",
            "legendFormat": "Used"
          },
          {
            "expr": "sum(jvm_memory_committed_bytes{environment=\"stagingkub\", area=\"nonheap\"}) / 1024 / 1024",
            "refId": "B",
            "legendFormat": "Committed"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": { "mode": "palette-classic" },
            "custom": {
              "axisCenteredZero": false,
              "axisColorMode": "text",
              "axisLabel": "Memory (MB)",
              "axisPlacement": "auto",
              "drawStyle": "line",
              "fillOpacity": 10,
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "showPoints": "never",
              "spanNulls": true,
              "stacking": { "group": "A", "mode": "none" },
              "thresholdsStyle": { "mode": "off" }
            },
            "unit": "decmbytes",
            "decimals": 0
          }
        },
        "options": {
          "legend": { "showLegend": true, "displayMode": "table", "placement": "right", "calcs": ["mean", "max"] },
          "tooltip": { "mode": "multi", "sort": "desc" }
        }
      },
      {
        "id": 13,
        "title": "JVM Heap par Pool",
        "type": "timeseries",
        "datasource": { "type": "prometheus", "uid": "prometheus" },
        "gridPos": { "x": 0, "y": 14, "w": 12, "h": 8 },
        "targets": [
          {
            "expr": "jvm_memory_used_bytes{environment=\"stagingkub\", area=\"heap\"} / 1024 / 1024",
            "refId": "A",
            "legendFormat": "{{id}}"
