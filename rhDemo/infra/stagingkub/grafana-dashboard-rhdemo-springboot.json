          },
          {
            "expr": "histogram_quantile(0.99, sum by (le) (rate(http_server_requests_seconds_bucket{environment=\"stagingkub\"}[5m])))",
            "refId": "C",
            "legendFormat": "p99"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": { "mode": "palette-classic" },
            "custom": {
              "axisCenteredZero": false,
              "axisColorMode": "text",
