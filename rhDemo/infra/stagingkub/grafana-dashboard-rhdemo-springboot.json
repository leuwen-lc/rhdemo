          }
        },
        "options": {
          "legend": { "showLegend": true, "displayMode": "table", "placement": "right", "calcs": ["mean", "max"] },
          "tooltip": { "mode": "multi", "sort": "desc" }
        }
      },
      {
        "id": 20,
        "title": "JVM Threads",
        "type": "row",
        "gridPos": { "x": 0, "y": 22, "w": 24, "h": 1 },
        "collapsed": false
      },
