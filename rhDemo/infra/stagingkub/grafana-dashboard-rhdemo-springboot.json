              "showPoints": "never",
              "spanNulls": true,
              "stacking": { "group": "A", "mode": "none" },
              "thresholdsStyle": { "mode": "off" }
            },
            "unit": "s",
            "decimals": 3
          }
        },
        "options": {
          "legend": { "showLegend": true, "displayMode": "table", "placement": "right", "calcs": ["mean", "max"] },
          "tooltip": { "mode": "multi", "sort": "desc" }
        }
